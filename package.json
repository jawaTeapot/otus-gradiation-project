    "sass": "^1.69.6",
    "sass-loader": "^13.3.3",
    "typescript": "^5.3.3",
    "vue": "^3.4.4",
    "vue-router": "^4.2.5",
    "vue-tsc": "^1.8.27"
  },
  "dependencies": {
    "@element-plus/nuxt": "^1.0.7",
    "@nuxt-alt/auth": "^2.5.0",
    "@nuxt-alt/http": "^1.6.4",
    "@nuxt/image": "^1.1.0",
    "@nuxtjs/apollo": "^5.0.0-alpha.6",
    "@nuxtjs/color-mode": "^3.3.2",
