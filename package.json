{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "nuxt start",
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "production": "rm -rf node_modules && NODE_ENV=production yarn install --prefer-offline --pure-lockfile --non-interactive --production=true",
    "clean": "rimraf node_modules .nuxt .output yarn.lock",
    "clean-cache": "yarn cache clean --all && npx nuxi cleanup",
    "test": "vitest run",
    "coverage": "vitest run --coverage"
  },

  "pre-commit": ["lint", "build", "test"],

  "devDependencies": {
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/test-utils": "^3.11.0",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@pinia/testing": "^0.1.3",
    "@types/vue-tel-input": "^2.1.6",
    "@vitest/coverage-v8": "^1.3.1",
    "@vue/test-utils": "^2.4.4",
    "eslint": "^8.56.0",
    "happy-dom": "^13.6.2",
    "nuxt": "^3.10.3",
    "playwright-core": "^1.41.2",
    "pre-commit": "^1.2.2",
    "sass": "^1.70.0",
    "sass-loader": "^14.1.0",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1",
    "vue": "^3.4.20",
    "vue-router": "^4.3.0",
    "vue-tsc": "^1.8.27"
  },
  "dependencies": {
    "@element-plus/nuxt": "^1.0.7",
    "@nuxt-alt/auth": "^3.1.6",
    "@nuxt-alt/http": "^1.7.10",
    "@nuxt/image": "^1.3.0",
    "@nuxtjs/apollo": "^5.0.0-alpha.11",
    "@nuxtjs/color-mode": "^3.3.2",
    "@nuxtjs/eslint-module": "^4.1.0",
    "@nuxtjs/i18n": "^8.0.0-beta.13",
    "@nuxtjs/tailwindcss": "^6.11.4",
    "@pinia/nuxt": "^0.5.1",
    "@vueuse/core": "^10.8.0",
    "@vueuse/integrations": "^10.8.0",
    "@vueuse/nuxt": "^10.8.0",
    "chart.js": "^4.4.1",
    "element-plus": "^2.5.6",
    "jwt-decode": "^4.0.0",
    "nuxt-icon": "^0.6.8",
    "pinia": "^2.1.7",
    "universal-cookie": "^7.1.0",
    "vue-chartjs": "^5.3.0",
    "vue-tel-input": "^8.3.1"
  }
}
